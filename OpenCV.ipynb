   "source": [
    "def labelset(img, xs, ys, label):\n",
    "    height, width = img.shape\n",
    "    img[ys, xs] = label\n",
    "    while True:\n",
    "        cnt = 0\n",
    "        \n",
    "        for y in range(1, height-1):\n",
    "            for x in range(1, width-1):\n",
    "                if img[y,x] == label:\n",
    "                    if img[y, x+1]  == 255:\n",
    "                        img[y, x+1]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y-1, x+1]  == 255:\n",
    "                        img[y-1, x+1]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y-1, x]  == 255:\n",
    "                        img[y-1, x]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y-1, x-1]  == 255:\n",
    "                        img[y-1, x-1]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y, x-1]  == 255:\n",
    "                        img[y, x-1]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y+1, x-1]  == 255:\n",
    "                        img[y+1, x-1]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y+1, x]  == 255:\n",
    "                        img[y+1, x]  = label\n",
    "                        cnt = cnt+1\n",
    "                    if img[y+1, x+1]  == 255:\n",
    "                        img[y+1, x+1]  = label\n",
    "                        cnt = cnt+1\n",
    "                        \n",
    "        if cnt==0:\n",
    "            return (0,img)\n",
    "    return (1,img)"
   ]
  },

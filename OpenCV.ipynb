    "def calc_size(image_label, label):\n",
    "    tx, ty = 0, 0\n",
    "    cv, cy = 0, 0\n",
    "    total = 0\n",
    "    height, width = image_label.shape\n",
    "    for y in range(height):\n",
    "        for x in range(width):\n",
    "            if image_label[y,x] == label:\n",
    "                tx = tx + x\n",
    "                ty = ty + y\n",
    "                total = total +1\n",
    "    if total == 0:\n",
    "        return 0\n",
    "    cx = int(tx/total)\n",
    "    cy = int(ty/total)\n",
    "    \n",
    "    return (cx, cy, total)"
   ]
